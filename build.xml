            <fileset dir="${dir.workspace}/bin"/>
        </jar>
        <delete dir="${dir.jarfile}/Enamel_lib"/>
        <mkdir dir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/lib/assertj-core-3.6.2.jar" todir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/lib/freetts.jar" todir="${dir.jarfile}/Enamel_lib"/>
