        <copy file="${dir.workspace}/pi4jWindows/pi4j-gpio-extension.jar" todir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/pi4jWindows/pi4j-service-javadoc.jar" todir="${dir.jarfile}/Enamel_lib"/>
