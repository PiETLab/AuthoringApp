        <copy file="${dir.workspace}/lib/cmulex.jar" todir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/lib/cmu_us_kal.jar" todir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/lib/cmudict04.jar" todir="${dir.jarfile}/Enamel_lib"/>
        <copy file="${dir.workspace}/lib/commons-io-2.5.jar" todir="${dir.jarfile}/Enamel_lib"/>
